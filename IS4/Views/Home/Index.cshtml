<header>
    <link rel="stylesheet" type="text/css" href="~/css/Custom.css"/>
</header>
<h1>Home page</h1>
@if (User.Identity != null)
{
    <h2>@User.Identity.Name</h2>
}
@foreach (var claim in User.Claims)
{
    <p>@claim</p>
}
<div style="width:100px; height:100px">
    <form style="width:100%; height:100%; color:antiquewhite" asp-controller="auth" asp-action="Login" asp-route-returnUrl="home/index" method="get">
        <button style="color:mediumaquamarine; width:100%; height:50%;">Login</button>
    </form>
</div>
